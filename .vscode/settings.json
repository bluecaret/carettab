{
  "i18n-ally.enabledFrameworks": ["chrome-ext", "vue"],
  "i18n-ally.extract.keygenStyle": "camelCase",
  "i18n-ally.theme.annotation": "rgba(200, 200, 255, 1)",
  "i18n-ally.theme.annotationBorder": "rgba(100, 100, 180, .7)",
  "i18n-ally.theme.annotationMissing": "rgba(210, 100, 100, .8)",
  "i18n-ally.theme.annotationMissingBorder": "rgba(210, 100, 100, .8)",
  "i18n-ally.annotations": false,
  "i18n-ally.annotationMaxLength": 40,
  "i18n-ally.defaultNamespace": "",
  "i18n-ally.translate.deepl.apiKey": "46ed014e-2a22-0b8b-05e3-ce56219e91f5:fx",
  "i18n-ally.sourceLanguage": "en",
  "i18n-ally.displayLanguage": "en",
  "i18n-ally.translate.engines": ["deepl", "google"],
  "i18n-ally.translate.deepl.useFreeApiEntry": true,
  "i18n-ally.localesPaths": ["./src/locales"],
  "i18n-ally.pathMatcher": "{locale}.json",
  "i18n-ally.review.enabled": false,
  "i18n-ally.keystyle": "nested",
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "stylelint.validate": ["css", "less", "postcss", "vue"],
  "json.schemas": [
    {
      "fileMatch": ["manifest.json"],
      "url": "https://json.schemastore.org/chrome-manifest.json"
    }
  ],
  "explorer.fileNesting.patterns": {
    "*Widget.vue": "$(capture).js, $(capture)*.vue, $(capture)*.scss, $(capture)*.css, $(capture)*.html",
    "*Tool.vue": "$(capture).js, $(capture)*.vue, $(capture)*.scss, $(capture)*.css, $(capture)*.html"
  }
}
